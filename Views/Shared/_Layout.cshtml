@using CentralTask.Models
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    @{
        ViewBag.Title = "";
    }
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <!-- Fav Icon -->
    <link rel="Shortcut Icon" href="~/Content/themes/mws/images/favicon.png" type="image/x-png">	

    <!-- Apple iOS and Android stuff (do not remove) -->
    <meta name="apple-mobile-web-app-capable" content="no" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no,maximum-scale=1" />
    <!-- Required Stylesheets -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/reset.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/text.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/fonts/ptsans/stylesheet.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/fluid.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/mws.style.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/icons/16x16.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/icons/24x24.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/icons/32x32.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/icons/button-icons.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/rm-multiselect.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/rm-multiselectListaUm.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/rm-multiselectListaDois.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/colorpicker/colorpicker.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/imgareaselect/css/imgareaselect-default.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/fullcalendar/fullcalendar.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/fullcalendar/fullcalendar.print.css")" media="print" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/chosen/chosen.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/prettyphoto/css/prettyPhoto.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/tipsy/tipsy.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/sourcerer/Sourcerer-1.2.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/jgrowl/jquery.jgrowl.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/spinner/ui.spinner.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/multiselect/jquery.multiselect.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/jui/css/jquery.ui.all.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/rm-alert/rm.alert.css")" media="screen" />
    
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/multiselect_checkbox/jquery.multiselect.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Scripts/plugins/multiselect_checkbox/jquery.multiselect.filter.css")" media="screen" />
    

    <!-- Theme Stylesheet -->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/themes/mws/css/mws.theme.css")" media="screen" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Site.css")" media="screen" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.maskedinput-1.3.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/plugins/spinner/ui.spinner.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/rm-multiselect.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/rm-multiselectListaUm.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/rm-multiselectListaDois.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/rm-multiselect-extended.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/plugins/multiselect/jquery.multiselect.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.jeditable.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/plugins/multiselect_checkbox/jquery.multiselect.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/plugins/multiselect_checkbox/jquery.multiselect.filter.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/toDictionaryPlugin.js")"></script>
    

    
    <!-- Plugin Scripts -->
    <!-- Core Script -->
    <!-- Themer Script (Remove if not needed) -->
    <script type="text/javascript" src="@Url.Content("~/Scripts/core/themer.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/function.js")"></script>

    <script type="text/javascript" src="@Url.Content("~/Scripts/Views/Home.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/plugins/rm-alert/rm.alert.js")"></script>

    <script>
        $(document).ready(function () {
            if (!String.prototype.trim) {
                String.prototype.trim = function () {
                    return this.replace(/^\s*([\S\s]*?)\s*$/, '$1');
                };
            }
            $.rmMultiselect('.multiselect', null);
            $('.datepicker').datepicker({ dateFormat: 'dd/mm/yy' });
            $('.datepicker').mask("?99/99/9999");
            $('.required').change(function () {
                $(this).valid();
            });
        });
    </script>

    <script>
        $(document).ready(function () {


        });
    </script>

    <title>@ViewBag.Title</title>
</head>
<body>
    @{
            <!-- Header -->
            <div id="mws-header" class="clearfix">
                <!-- Logo Container -->
                <div id="mws-logo-container">
                    <!-- Logo Wrapper, images put within this wrapper will always be vertically centered -->
                    <div id="mws-logo-wrap">
                        <img src="@Url.Content("~/images/logo-Perfil.png")" alt="mws admin" />
                    </div>
                </div>
            </div>
            <div id="mws-wrapper">
                <div id="mws-container" class="clearfix">
                    <!-- Inner Container Start -->
                    <div class="container">
                        <div>
                            @RenderBody()
                        </div>
                        <div class="div-container-loading-back  loading-save">
                            <div class="div-container-loading">
                            </div>
                        </div>
                    </div>
                    <!-- Inner Container End -->
                    <!-- Footer -->
                    <div id="mws-footer">
                    </div>

                </div>
            </div>
        }
        </body>
</html>
